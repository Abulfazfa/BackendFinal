@model AboutSection
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/AdminArea/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">

                    <div class="form-group">
                        <h2>@Model.Title</h2>
                    </div>
                    <div class="form-group">
                        <p>@Model.Description</p>
                    </div>

                    <a class="btn btn-dark" asp-controller="adminabout" asp-action="index">Back</a>
            </div>
        </div>
    </div>
</div>


